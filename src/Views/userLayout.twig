<html>
<head> 
    <meta charset="UTF-8"> 
    <title>{% block title %}{% endblock %}</title>
    <style> 
    .papa {
        width: 40vw;
        height: 70vh;
        position: absolute;
        top: 20vh;
        left: 30vw;
    }

    .kiddo {
        float: right;

    }
    </style>
    {% block styles %}
    {% endblock %}
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
</head>
<body>
    <div class="papa">
    {% block content %}{% endblock %}
    </div>

    <div class="kiddo">
    {% block rightFloat %}{% endblock %}

    {% block scripts %}
    {% endblock %}
</body>
</html>